package _2577;

import java.util.Scanner;

public class _2577 {
	
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		
		int A = scan.nextInt();
		int B = scan.nextInt();
		int C = scan.nextInt();
		String num = String.valueOf(A*B*C);
		int i;
		
		if(num.indexOf("0") = i) {
			System.out.println(i);
		}
		
	}

}
